{
  "name": "ngx-smart-popover",
  "version": "1.5.0",
  "resolutions": {
    "normalize-url": "6.1.0",
    "serialize-javascript": "6.0.0",
    "trim-newlines": "3.0.1"
  },
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:all": "ng build --configuration production && npm run build:lib",
    "build:lib": "ng build ngx-smart-popover --configuration production  && npm run build:lib:scss && npm run build:lib:css",
    "build:lib:css": "sass ./projects/ngx-smart-popover/src/lib/popover-content.component.scss ./dist/ngx-smart-popover/css/popover-content.component.css",
    "build:lib:scss": "mkdir ./dist/ngx-smart-popover/scss/ && cp ./projects/ngx-smart-popover/src/lib/popover-content.component.scss ./dist/ngx-smart-popover/scss/popover-content.component.scss",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:entrypoint": "npm install && ng e2e",
    "e2e:docker": "docker build -t ngx-smart-popover-e2e-tests . && docker run -v ~/code/ngx-smart-popover:/usr/src/app --rm ngx-smart-popover-e2e-tests",
    "run": "ng-packagr -p package.json",
    "publish": "cp README.md dist/ngx-smart-popover/README.md && cd dist/ngx-smart-popover && npm publish"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "18.2.11",
    "@angular/common": "18.2.11",
    "@angular/compiler": "18.2.11",
    "@angular/core": "^18.2.11",
    "@angular/forms": "18.2.11",
    "@angular/platform-browser": "18.2.11",
    "@angular/platform-browser-dynamic": "18.2.11",
    "@angular/router": "18.2.11",
    "bootstrap": "4.3.1",
    "core-js": "2.6.4",
    "rxjs": "6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "^0.14.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.11",
    "@angular/cli": "^18.2.11",
    "@angular/compiler-cli": "18.2.11",
    "@angular/language-service": "18.2.11",
    "@types/jasmine": "^3.8.0",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "^3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^18.2.1",
    "normalize-url": "6.1.0",
    "protractor": "~7.0.0",
    "protractor-jasmine2-screenshot-reporter": "0.5.0",
    "sass": "^1.51.0",
    "scss-bundle": "3.1.2",
    "serialize-javascript": "6.0.0",
    "trim-newlines": "3.0.1",
    "ts-node": "7.0.1",
    "tslint": "~6.1.0",
    "typescript": ">=5.4.0 <5.6.0"
  }
}
